<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="Smpte2109">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ContainerConfig"/>
        <xs:element ref="ProfessionalMetadata"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ContainerConfig">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SampleOffset"/>
        <xs:element ref="DynamicTags"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="SampleOffset">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="-32768"/>
        <xs:maxInclusive value="32767"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="DynamicTags">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="Tag">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="TagReplacement">
                <xs:attribute name="id" use="required" type="DynamicTagId"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="TagReplacement">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-fA-F]{2}(\.[0-9a-fA-F]{2}){15}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DynamicTagId">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-fA-F]+"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="PmdString">
    <xs:restriction base="xs:string">
        <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ProfessionalMetadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="1" name="Title" type="PmdString"/>
        <xs:element minOccurs="1" maxOccurs="1" ref="AudioSignals"/>
        <xs:element minOccurs="1" maxOccurs="1" ref="AudioElements"/>
        <xs:element minOccurs="1" maxOccurs="1" ref="Presentations"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="PresentationLoudness"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="EncoderConfigurations"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DynamicUpdate"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="IAT"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="HeadphoneElements"/>
      </xs:sequence>
      <xs:attribute name="version" use="required" type="VersionType"/>
      <xs:attribute name="profile_number" use="optional" type="ProfileInt"/>
      <xs:attribute name="profile_level"  use="optional" type="ProfileInt"/>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="ProfileInt">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="VersionType">
    <xs:restriction base="PmdString">
      <xs:pattern value="[0-9]+\.[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- ___ Audio Signals _______________________________________________________________ -->

  <xs:element name="AudioSignals">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="255" name="AudioSignal" type="AudioSignalDef"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AudioSignalDef">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Name" type="PmdString"/>
    </xs:sequence>
    <xs:attribute name="id" use="required" type="AudioSignalIdType"/>
  </xs:complexType>

  <xs:simpleType name="AudioSignalIdType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="255"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- __ Audio Elements __________________________________________________ -->

  <xs:element name="AudioElements">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="AudioBed"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="AudioObject"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="AudioBed">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" name="Name" type="PmdString"/>
        <xs:element minOccurs="1" maxOccurs="1" name="SpeakerConfig" type = "SpeakerConfigType"/>
	<xs:element minOccurs="0" maxOccurs="1" name="SourceBedId" type = "AudioElementIdType"/>
        <xs:element ref="OutputTargets"/>
      </xs:sequence>
      <xs:attribute name="id" use="required" type="AudioElementIdType"/>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="AudioElementIdType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="4095"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SpeakerConfigType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="2.0"/>
      <xs:enumeration value="3.0"/>
      <xs:enumeration value="5.1"/>
      <xs:enumeration value="5.1.2"/>
      <xs:enumeration value="5.1.4"/>
      <xs:enumeration value="7.1.4"/>
      <xs:enumeration value="9.1.6"/>
      <xs:enumeration value="Portable Speaker"/>
      <xs:enumeration value="Portable Headphone"/>
      <xs:enumeration value="Not Indicated"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SpeakerType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Left"/>
      <xs:enumeration value="Right"/>
      <xs:enumeration value="Center"/>
      <xs:enumeration value="LFE"/>
      <xs:enumeration value="Left Surround"/>
      <xs:enumeration value="Right Surround"/>
      <xs:enumeration value="Left Rear Surround"/>
      <xs:enumeration value="Right Rear Surround"/>
      <xs:enumeration value="Left Top Front"/>
      <xs:enumeration value="Right Top Front"/>
      <xs:enumeration value="Left Top Middle"/>
      <xs:enumeration value="Right Top Middle"/>
      <xs:enumeration value="Left Top Rear"/>
      <xs:enumeration value="Right Top Rear"/>
      <xs:enumeration value="Left Front Wide"/>
      <xs:enumeration value="Right Front Wide"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="OutputTargets">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="OutputTarget"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="OutputTarget">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AudioSignals">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="1" maxOccurs="unbounded" name="ID">
                <xs:complexType>
      <xs:simpleContent>
          <xs:extension base="AudioSignalIdType">
          <xs:attribute name="source_gain_db" type="Gain"/>
        </xs:extension>
      </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="id" use="required" type="SpeakerType"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="AudioObject">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" name="Name"           type="PmdString"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Class"          type="ClassEnum"/>
        <xs:element minOccurs="1" maxOccurs="1" name="DynamicUpdates" type="Boolean"/>
        <xs:element minOccurs="1" maxOccurs="1" name="X_Pos"          type="Coordinate"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Y_Pos"          type="Coordinate"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Z_Pos"          type="Coordinate"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Size"           type="UnitFloat"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Size_3D"        type="Boolean"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Diverge"        type="Boolean"/>
        <xs:element minOccurs="1" maxOccurs="1" name="AudioSignal"    type="AudioSignalIdType"/>
        <xs:element minOccurs="1" maxOccurs="1" name="SourceGainDB"   type="Gain"/>
      </xs:sequence>
      <xs:attribute name="id" use="required" type="AudioElementIdType"/>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="ClassEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Dialog"/>
      <xs:enumeration value="VDS"/>
      <xs:enumeration value="Voice Over"/>
      <xs:enumeration value="Spoken Subtitle"/>
      <xs:enumeration value="Emergency Alert"/>
      <xs:enumeration value="Emergency Information"/>
      <xs:enumeration value="Generic"/>
      <xs:enumeration value="Reserved"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Boolean">
    <xs:restriction base="xs:string">
      <xs:enumeration value="False"/>
      <xs:enumeration value="True"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="UnitFloat">
    <xs:restriction base="xs:float">  
      <xs:minInclusive value="0.0"/>
      <xs:maxInclusive value="1.0"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Coordinate">
    <xs:restriction base="xs:float">  
      <xs:minInclusive value="-1.0"/>
      <xs:maxInclusive value="1.0"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Gain">
    <xs:restriction base="xs:string">
      <xs:enumeration value="-infdB"/>
      <xs:enumeration value="-25.0dB"/>
      <xs:enumeration value="-24.5dB"/>      
      <xs:enumeration value="-24.0dB"/>
      <xs:enumeration value="-23.5dB"/>
      <xs:enumeration value="-23.0dB"/>
      <xs:enumeration value="-22.5dB"/>
      <xs:enumeration value="-22.0dB"/>
      <xs:enumeration value="-21.5dB"/>
      <xs:enumeration value="-21.0dB"/>
      <xs:enumeration value="-20.5dB"/>
      <xs:enumeration value="-20.0dB"/>
      <xs:enumeration value="-19.5dB"/>
      <xs:enumeration value="-19.0dB"/>
      <xs:enumeration value="-18.5dB"/>
      <xs:enumeration value="-18.0dB"/>
      <xs:enumeration value="-17.5dB"/>
      <xs:enumeration value="-17.0dB"/>
      <xs:enumeration value="-16.5dB"/>
      <xs:enumeration value="-16.0dB"/>
      <xs:enumeration value="-15.5dB"/>
      <xs:enumeration value="-15.0dB"/>
      <xs:enumeration value="-14.5dB"/>
      <xs:enumeration value="-14.0dB"/>
      <xs:enumeration value="-13.5dB"/>
      <xs:enumeration value="-13.0dB"/>
      <xs:enumeration value="-12.5dB"/>
      <xs:enumeration value="-12.0dB"/>
      <xs:enumeration value="-11.5dB"/>
      <xs:enumeration value="-11.0dB"/>
      <xs:enumeration value="-10.5dB"/>
      <xs:enumeration value="-10.0dB"/>
      <xs:enumeration value="-9.5dB"/>
      <xs:enumeration value="-9.0dB"/>
      <xs:enumeration value="-8.5dB"/>
      <xs:enumeration value="-8.0dB"/>
      <xs:enumeration value="-7.5dB"/>
      <xs:enumeration value="-7.0dB"/>
      <xs:enumeration value="-6.5dB"/>
      <xs:enumeration value="-6.0dB"/>
      <xs:enumeration value="-5.5dB"/>
      <xs:enumeration value="-5.0dB"/>
      <xs:enumeration value="-4.5dB"/>
      <xs:enumeration value="-4.0dB"/>
      <xs:enumeration value="-3.5dB"/>
      <xs:enumeration value="-3.0dB"/>
      <xs:enumeration value="-2.5dB"/>
      <xs:enumeration value="-2.0dB"/>
      <xs:enumeration value="-1.5dB"/>
      <xs:enumeration value="-1.0dB"/>
      <xs:enumeration value="-0.5dB"/>
      <xs:enumeration value="0.0dB"/>
      <xs:enumeration value="0.5dB"/>
      <xs:enumeration value="1.0dB"/>
      <xs:enumeration value="1.5dB"/>
      <xs:enumeration value="2.0dB"/>
      <xs:enumeration value="2.5dB"/>
      <xs:enumeration value="3.0dB"/>
      <xs:enumeration value="3.5dB"/>
      <xs:enumeration value="4.0dB"/>
      <xs:enumeration value="4.5dB"/>
      <xs:enumeration value="5.0dB"/>
      <xs:enumeration value="5.5dB"/>
      <xs:enumeration value="6.0dB"/>
    </xs:restriction>
  </xs:simpleType>
  
  <!-- __ Headphone Elements __________________________________________________ -->

  <xs:element name="HeadphoneElements">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="HeadphoneElement"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="HeadphoneElement">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="1" name="Element" type="AudioElementIdType"/>
        <xs:element minOccurs="1" maxOccurs="1" name="HeadTrackingEnabled" type="Boolean"/>
        <xs:element minOccurs="1" maxOccurs="1" name="RenderMode" type="RenderModeType"/>
        <xs:element minOccurs="0" maxOccurs="1" name="ChannelExclusions" type="ChannelExclusionsType"/>
      </xs:sequence>
      <xs:attribute name="id" type="AudioElementIdType"/>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="RenderModeType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="127"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="ChannelExclusionsType">
    <xs:sequence>
       <xs:element maxOccurs="unbounded" name="ID" type="SpeakerType"/>
    </xs:sequence>
  </xs:complexType>

  <!-- _____ Presentations ________________________________________________ -->

  <xs:element name="Presentations">
     <xs:complexType>
       <xs:sequence>
         <xs:element minOccurs="1" maxOccurs="511" name="Presentation" type="PresentationDef"/>
       </xs:sequence>
     </xs:complexType>   
  </xs:element>

  <xs:simpleType name="PresentationIdType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="511"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="PresentationDef">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="16" name="Name">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="PmdString">
              <xs:attribute name="language" use="required" type="CountryCode"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="Config"       type="PresentationConfig"/>
      <xs:element minOccurs="1" maxOccurs="1" name="Language"     type="CountryCode"/>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Element"      type="AudioElementIdType"/>
    </xs:sequence>
    <xs:attribute name="id" use="required" type="PresentationIdType"/>
  </xs:complexType>  

  <xs:simpleType name="PresentationConfig">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="(((2\.0)|(3\.0)|(5\.1(\.(2|4))?)|(7\.1\.4)|(9\.1\.6))( (ME|CM))?( ?\+ [0-9]*(D|VDS|VO|SS|O|EI|EA))*)|(Portable (Headphone|Speaker))"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- ______ Presentation Loudness ______________________________________________-->

  <xs:element name="PresentationLoudness">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" name="Presentation" type="Loudness"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="Loudness">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="PresentationId"        type="PresentationIdType"/>
      <xs:element minOccurs="0" maxOccurs="1" name="PracticeType"          type="PracticeTypeType"/>
      <xs:element minOccurs="0" maxOccurs="1" name="LoudnessRelativeGated" type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="LoudnessSpeechGated"   type="LoudnessSpeechGatedType"/>
      <xs:element minOccurs="0" maxOccurs="1" name="Loudness3Seconds"      type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="MaxLoudness3Seconds"   type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="TruePeak"              type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="MaxTruePeak"           type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="ProgramBoundary"       type="ProgramBoundaryType"/>
      <xs:element minOccurs="0" maxOccurs="1" name="LoudnessRange"         type="LoudnessRangeType"/>
      <xs:element minOccurs="0" maxOccurs="1" name="MomentaryLoudness"     type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="MaxMomentaryLoudness"  type="LUFS"/>
      <xs:element minOccurs="0" maxOccurs="1" name="Extension"             type="LoudnessExtensionType"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="LoudnessSpeechGatedType">
    <xs:simpleContent>    
      <xs:extension base="LUFS">
        <xs:attribute name="dialgate" type="DialgateType"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="PracticeTypeType">
    <xs:simpleContent>
      <xs:extension base="PracticeTypeEnum">
        <xs:attribute name="dialgate" type="DialgateType"/>
        <xs:attribute name="correction_type" use="required" type="CorrectionTypeType"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  
  <xs:simpleType name="PracticeTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NI"/>
      <xs:enumeration value="ATSC"/>
      <xs:enumeration value="EBU"/>
      <xs:enumeration value="ARIB"/>
      <xs:enumeration value="FreeTV"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
      <xs:enumeration value="10"/>
      <xs:enumeration value="11"/>
      <xs:enumeration value="12"/>
      <xs:enumeration value="13"/>
      <xs:enumeration value="Manual"/>
      <xs:enumeration value="Consumer"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DialgateType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NI"/>
      <xs:enumeration value="Center"/>
      <xs:enumeration value="Front"/>
      <xs:enumeration value="Manual"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CorrectionTypeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="file"/>
      <xs:enumeration value="realtime"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="LUFS">
    <xs:restriction base="xs:float">  
      <xs:minInclusive value="-102.4"/>
      <xs:maxInclusive value="102.3"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="ProgramBoundaryType">
    <xs:simpleContent>
       <xs:extension base="ProgramBoundaryValue">
         <xs:attribute name="offset" type="ProgramBoundaryOffsetType"/>
       </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="ProgramBoundaryValue">
    <xs:restriction base="xs:integer">
      <xs:enumeration value="-512"/>
      <xs:enumeration value="-256"/>
      <xs:enumeration value="-128"/>
      <xs:enumeration value="-64"/>
      <xs:enumeration value="-32"/>
      <xs:enumeration value="-16"/>
      <xs:enumeration value="-8"/>
      <xs:enumeration value="-4"/>
      <xs:enumeration value="-2"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="16"/>
      <xs:enumeration value="32"/>
      <xs:enumeration value="64"/>
      <xs:enumeration value="128"/>
      <xs:enumeration value="256"/>
      <xs:enumeration value="512"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ProgramBoundaryOffsetType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="2047"/>
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="LoudnessRangeType">
    <xs:simpleContent>
      <xs:extension base="LoudnessRangeValue">
        <xs:attribute name="practice" use="required" type="LRAPracType"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="LoudnessRangeValue">
    <xs:restriction base="xs:float">
      <xs:minInclusive value="0.0"/>
      <xs:maxInclusive value="102.3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LRAPracType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="7"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="LoudnessExtensionType">
    <xs:complexContent>
      <xs:extension base="codedstring">
        <xs:attribute name="bits" type="LoudnessExtensionBitsType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LoudnessExtensionBitsType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="2048"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- _____ Encoder configurations ______________________________________________ -->

  <xs:element name="EncoderConfigurations">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Eac3EncodingParameters"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ED2Turnaround"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <!-- _____ Eac3 encoding parameters ____________________________________________ -->

  <xs:element name="Eac3EncodingParameters">
    <xs:complexType>
       <xs:sequence>      
         <xs:element minOccurs="0" maxOccurs="1" name="Name"          type="PmdString"/>
         <xs:element minOccurs="0" maxOccurs="1" name="Encoder"       type="Eac3EncoderParamsType"/>
         <xs:element minOccurs="0" maxOccurs="1" name="Bitstream"     type="Eac3BitstreamParamsType"/>
         <xs:element minOccurs="0" maxOccurs="1" name="DRC"           type="Eac3DrcParamsType"/>
         <xs:element minOccurs="0" maxOccurs="1" name="Presentations" type="Eac3PresentationsParamsType"/>
