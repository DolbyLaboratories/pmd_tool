  <xs:simpleType name="UUIDContentType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="EIDRContentType">
    <xs:restriction base="xs:string">
      <xs:pattern value="(10\.)?5240/?[0-9a-fA-F]{4}-?[0-9a-fA-F]{4}-?[0-9a-fA-F]{4}-?[0-9a-fA-F]{4}-?[0-9a-fA-F]{4}-?C?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ADIdContentType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-zA-Z]{11}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="RawContentType">
